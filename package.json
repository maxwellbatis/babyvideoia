{
  "name": "unified-video-generator",
  "version": "1.0.0",
  "main": "server.ts",
  "scripts": {
    "start": "ts-node server.ts",
    "dev": "ts-node-dev --respawn server.ts",
    "generate": "ts-node src/orchestrators/orchestrator-unified.ts",
    "generate:clips": "ts-node src/orchestrators/orchestrator-unified.ts --formato clips",
    "generate:styled": "ts-node src/orchestrators/orchestrator-unified.ts --formato styled",
    "generate:aiimage": "ts-node src/orchestrators/orchestrator-animated-images.ts",
    "generate:images": "ts-node src/orchestrators/orchestrator-images.ts",
    "generate:kling": "ts-node src/orchestrators/orchestrator-kling.ts",
    "generate:faceless": "ts-node src/orchestrators/orchestrator-unified.ts --formato faceless",
    "generate:aivideo": "ts-node src/orchestrators/orchestrator-unified.ts --formato aivideo",
    "generate:klingvideo": "ts-node src/orchestrators/orchestrator-unified.ts --formato klingvideo",
    "server": "ts-node server.ts",
    "build": "tsc",
    "test": "echo \"No tests specified\" && exit 0",
    "test:fallback": "ts-node test-fallback.js",
    "test:video-types": "ts-node test-video-types.js",
    "clean": "rimraf dist output/tmp",
    "setup": "npm install && pip install -r requirements.txt"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.11.1",
    "@types/multer": "^2.0.0",
    "axios": "^1.10.0",
    "canvas": "^3.1.2",
    "cloudinary": "^2.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "ffmpeg-static": "^5.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "form-data": "^4.0.3",
    "groq-sdk": "^0.26.0",
    "image-size": "^2.0.2",
    "json5": "^2.2.3",
    "multer": "^2.0.1",
    "node-fetch": "^2.7.0",
    "openai": "^4.28.2",
    "pg": "^8.16.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/fs-extra": "^11.0.4",
    "@types/json5": "^0.0.30",
    "@types/node": "^24.0.12",
    "concurrently": "^8.2.2",
    "prisma": "^6.11.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
